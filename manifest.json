{
    "name": "Flash Search",
    "description": "Visiting your favorite websites has never been so fast",
    "version": "0.1",
    "manifest_version": 3,
    "options_page": "views/options/options.html",
    "chrome_url_overrides": {
        "newtab": "views/newtab/newtab.html"
    },
    "permissions": [
        "storage",
        "search",
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_popup": "views/popup/popup.html"
    }
}